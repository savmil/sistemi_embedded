#include "xil_types.h"
#include "xstatus.h"
#include "xil_io.h"

typedef struct{
	UINTPTR BaseAddress;
	UINTPTR IntAddress;
} myIntGPIO;


void MYIP_WITH_INTERRUPT_EnableInterrupt(myIntGPIO * provaIntInstance, u32 mask);
void MYIP_WITH_INTERRUPT_ACK(myIntGPIO * provaIntInstance, u32 mask);
void MYIP_WITH_INTERRUPT_GlobalEnableInterrupt(myIntGPIO * provaIntInstance, u32 mask);
void MYIP_WITH_INTERRUPT_DisableInterrupt(myIntGPIO * provaIntInstance, u32 mask);
void MYINTGPIO_SetMask(myIntGPIO * provaIntInstance, u32 mask);
