#ifndef SRC_UART_INTR_H_
#define SRC_UART_INTR_H_

#include "xil_types.h"
#include "xstatus.h"
#include "xil_io.h"

typedef struct{
	UINTPTR BaseAddress;
} uart_intr;


void MYIP_WITH_INTERRUPT_EnableInterrupt(uart_intr * provaIntInstance, u32 mask);
void MYIP_WITH_INTERRUPT_ACK(uart_intr * provaIntInstance, u32 mask);
void MYIP_WITH_INTERRUPT_GlobalEnableInterrupt(uart_intr * provaIntInstance, u32 mask);
void MYIP_WITH_INTERRUPT_DisableInterrupt(uart_intr * provaIntInstance, u32 mask);


#endif /* SRC_UART_INTR_H_ */
